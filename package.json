{
    "name": "journall",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "watch": "sass --watch scss/style.scss css/style.css",
        "compile": "sass scss/style.scss css/style.comp.css",
        "prefix": "postcss css/style.comp.css -u autoprefixer -o css/style.prefix.css",
        "compress": "sass css/style.prefix.css css/style.css --style compressed",
        "build": "npm-run-all compile prefix compress"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "autoprefixer": "^10.3.1",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^8.3.1",
        "sass": "^1.35.2"
    }
}